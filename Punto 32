#include <stdio.h>

int main(){
    int A, B;
    printf("Eliga un número para ver si es par o impar\n");
    printf("Ingrese su número: ");
    scanf("%d", &A);
    
    B=A%2;
    
    if(B==0){
        printf("Tu número es par");
    }else{
        printf("Tu número es impar");
    }
    return 0;
}
